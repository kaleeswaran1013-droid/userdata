				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"successfull response\",function(){\r",
							"pm.response.to.have.status(200)\r",
							"})\r",
							"\r",
							"pm.test(\"status code is 404\",function(){\r",
							"    pm.response.to.have.status('OK');\r",
							"});\r",
							"pm.test(\"successfull POST request \",function(){\r",
							"   pm.expect(pm.response.code).to.be.oneOf([2001,200]);\r",
							"});\r",
							"\r",
							"pm.test(\"expecting only Success response\",function(){\r",
							"    pm.expect(pm.response.code).to.be.within([201,299]);\r",
							"});\r",
							"\r",
							"pm.test(\"expecting only Success response\",function(){\r",
							"    pm.expect(pm.response.code).to.be.above(399);\r",
							"});\r",
							"\r",
							"pm.test(\"error pn client\",function(){\r",
							"    pm.expect(pm.response.code).to.be.below(500);\r",
							"});\r",
							"\r",
							"pm.test(\"response is fast \",()=>{\r",
							"    pm.expect(pm.response.responseTime).to.be.below(200);\r",
							"})\r",
							"\r",
							"pm.test(\"productID is found\",()=>{\r",
							"    pm.expect(pm.response.json()).to.have.property('productID')\r",
							"})\r",
							"\r",
							"let schema = {\r",
							"    \"type\" : \"object\",\r",
							"    \"required\" : ['productID','string'],\r",
							"    \"properties\":{\r",
							"    \"properties\":{\"type\":\"string\"},\r",
							"    \"stock\": { \"type\": \"string\"},\r",
							"    \"price\": {\"type\":\"string\"},\r",
							"    \"productID\":{\"type\": \"type\"} \r",
							"    }\r",
							"}\r",
							"pm.test(\"json scane check\",()=>{\r",
							"    pm.response.to.have.jsonSchema(schema)\r",
							"})\r",
							"pm.test('schema is valid',function(){\r",
							"    pm.expect(tv4.validata(pm.response.json(),schema)).to.be.true;\r",
							"});\r",
							"pm.collectionVariables.set(\"name\",pm.response.json().productName)\r",
							"pm.environment.set(\"price\",pm.response.json().price)\r",
							"pm.globals.set(\"stock\",pm.response.json().stock)\r",
							"console.log(pm.collectionVariables.get(\"name\"))\r",
							"console.log(pm.environment.get(\"price\"))\r",
							"console.log(pm.globals.get(\"stock\"))\r",
							"pm.globals.clear()"
						],
